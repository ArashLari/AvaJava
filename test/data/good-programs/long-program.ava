var mapFunction = function (f:function, l:list) ->
	var newList = [];
	for each x in l {
		push(newList, f x);
	};
	return newList;
end;

var addString = function (s:string) ->
	return s + '0';
end;

var x = [ 'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h' ];

var y = mapFunction(addString, x);

var z = { y[0], y[7] };

var index = 0;

while (index < length(y)) {
	ava y[index];
	index++;
};